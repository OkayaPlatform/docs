{
  "name": "@mintlify/cli",
  "version": "4.0.46",
  "description": "The Mintlify CLI",
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "Mintlify, Inc.",
  "bugs": {
    "url": "https://github.com/mintlify/docs/issues"
  },
  "license": "Elastic-2.0",
  "keywords": [
    "mintlify",
    "mint",
    "cli"
  ],
  "type": "module",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "exports": "./bin/index.js",
  "bin": {
    "mintlify": "bin/index.js"
  },
  "scripts": {
    "dev": "yarn build && node bin/index.js dev",
    "prepare": "npm run build",
    "build": "tsc --project tsconfig.build.json",
    "clean:build": "rimraf bin",
    "clean:all": "rimraf node_modules .eslintcache && yarn clean:build",
    "watch": "tsc --watch",
    "lint": "eslint src --cache",
    "test": "vitest run",
    "type": "tsc --noEmit",
    "format": "prettier . --write",
    "format:check": "prettier . --check"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "@mintlify/link-rot": "3.0.68",
    "@mintlify/models": "0.0.51",
    "@mintlify/prebuild": "1.0.68",
    "@mintlify/previewing": "4.0.45",
    "@mintlify/validation": "0.1.94",
    "axios": "^1.4.0",
    "chalk": "^5.2.0",
    "detect-port": "^1.5.1",
    "favicons": "^7.0.2",
    "fs-extra": "^11.1.1",
    "gray-matter": "^4.0.3",
    "inquirer": "^9.2.7",
    "is-absolute-url": "^4.0.1",
    "js-yaml": "^4.1.0",
    "openapi-types": "^12.0.0",
    "remark": "^14.0.2",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "remark-mdx": "^2.2.1",
    "unist-util-visit": "^4.1.1",
    "vfile": "^5.3.6",
    "yargs": "^17.6.0"
  },
  "devDependencies": {
    "@mintlify/eslint-config": "1.0.4",
    "@mintlify/eslint-config-typescript": "1.0.9",
    "@mintlify/prettier-config": "1.0.1",
    "@mintlify/ts-config": "2.0.1",
    "@trivago/prettier-plugin-sort-imports": "3.x",
    "@tsconfig/recommended": "1.x",
    "@types/detect-port": "^1.3.2",
    "@types/inquirer": "^9.0.1",
    "@types/node": "^18.7.13",
    "@types/yargs": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "6.x",
    "@typescript-eslint/parser": "6.x",
    "eslint": "8.x",
    "eslint-config-prettier": "8.x",
    "eslint-plugin-unused-imports": "3.x",
    "prettier": "2.x",
    "rimraf": "^5.0.1",
    "typescript": "^4.9.5",
    "vitest": "^0.34.6"
  },
  "gitHead": "4e76dca81bccf1d79c8439731205b89157c9c796"
}
