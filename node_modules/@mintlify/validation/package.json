{
  "name": "@mintlify/validation",
  "version": "0.1.94",
  "description": "Validates mint.json files",
  "author": "Mintlify, Inc.",
  "bugs": {
    "url": "https://github.com/mintlify/docs/issues"
  },
  "license": "Elastic-2.0",
  "keywords": [
    "mintlify",
    "mint",
    "validation"
  ],
  "type": "module",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc --project tsconfig.build.json",
    "clean:build": "rimraf dist",
    "clean:all": "rimraf node_modules .eslintcache && yarn clean:build",
    "test:type": "vitest typecheck --run",
    "test": "vitest run",
    "type": "tsc --noEmit",
    "lint": "eslint . --cache",
    "format": "prettier . --write",
    "format:check": "prettier . --check"
  },
  "dependencies": {
    "lcm": "^0.0.3",
    "zod": "^3.20.6",
    "zod-to-json-schema": "^3.20.3"
  },
  "peerDependencies": {
    "@mintlify/models": ">= 0.0.36 < 1",
    "openapi-types": "12.x"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@mintlify/eslint-config": "1.0.4",
    "@mintlify/eslint-config-typescript": "1.0.9",
    "@mintlify/models": "0.0.51",
    "@mintlify/prettier-config": "1.0.1",
    "@mintlify/ts-config": "2.0.1",
    "@trivago/prettier-plugin-sort-imports": "3.x",
    "@tsconfig/recommended": "1.x",
    "@types/lcm": "^0.0.0",
    "@typescript-eslint/eslint-plugin": "6.x",
    "@typescript-eslint/parser": "6.x",
    "eslint": "8.x",
    "eslint-config-prettier": "8.x",
    "eslint-plugin-unused-imports": "3.x",
    "openapi-types": "^12.1.0",
    "prettier": "2.x",
    "rimraf": "^5.0.1",
    "ts-expect": "^1.3.0",
    "ts-loader": "^9.4.2",
    "typescript": "^4.8.2",
    "vitest": "^0.34.6",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "gitHead": "4e76dca81bccf1d79c8439731205b89157c9c796"
}
